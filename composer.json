{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "akaunting/laravel-setting": "^1.2",
        "barryvdh/laravel-translation-manager": "^0.6.3",
        "brotzka/laravel-dotenv-editor": "^2.1",
        "cviebrock/eloquent-sluggable": "^9.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^3.4",
        "firebase/php-jwt": "^6.3",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "jackiedo/timezonelist": "^5.1",
        "laravel-notification-channels/twilio": "^3.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "livewire/livewire": "^2.10",
        "plank/laravel-mediable": "^5.9",
        "pragmarx/countries-laravel": "^0.7.0",
        "rachidlaasri/laravel-installer": "^4.1",
        "shipu/watchable": "dev-master",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-medialibrary": "^10.4.4",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sluggable": "^3.4",
        "tymon/jwt-auth": "*",
        "yajra/laravel-datatables": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helper/Helper.php",
            "app/Helpers/helpers.php",
            "app/Helpers/Curl.php",
            "app/Helpers/Ip.php",
            "app/Libraries/ArtisanLibrary.php",
            "app/Libraries/FileLibrary.php",
            "app/Libraries/MyString.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}